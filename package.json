{
  "name": "Star",
  "private": true,
  "scripts": {
    "start": "meteor",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "@cleverbeagle/seeder": "^1.3.0",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "buffer": "^5.0.8",
    "commonmark": "^0.28.1",
    "core-js": "^2.5.3",
    "handlebars": "^4.0.11",
    "js-md5": "^0.7.3",
    "juice": "^4.2.2",
    "lodash": "^4.17.4",
    "meteor-node-stubs": "^0.2.11",
    "moment": "^2.19.4",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "scrypt-js": "2.0.3",
    "setimmediate": "1.0.5",
    "simpl-schema": "0.3.2",
    "styled-components": "^3.2.6",
    "telegraf": "^3.20.0",
    "tweetnacl": "1.0.0",
    "tweetnacl-auth": "0.3.1",
    "tweetnacl-util": "0.15.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coffeescript": "2.1.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-meteor": "^4.1.6",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "extends": [
      "plugin:meteor/recommended",
      "plugin:react/recommended",
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "expect": false
    },
    "rules": {
      "indent": 4,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "import/no-absolute-path": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "max-len": 0,
      "no-alert": 0,
      "curly-newline": 0,
      "no-console": 0,
      "no-restricted-globals": 0,
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ],
      "class-methods-use-this": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0,
      "arrow-parens": 0,
      "no-nested-ternary": 0,
      "no-unused-expressions": 0,
      "no-absolute-path": 0
    },
    "settings": {
      "import/resolver": "meteor",
      "import/no-absolute-path": 0
    }
  }
}
